{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"lib": [
			"es2015",
			"dom"
		],
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"**/*.ts"
	],
	"exclude": [
		"node_modules"
	]
}